xi_1_func <- function(x, y){
  x*y/(x*y + (1-y)*(1-x))
}

x0 <- 5
xi <- 4





xi_2_func <- function(xi,x0){
  if (xi>=x0) 
    .5*((xi*(1-x0)/x0*(1-xi)) + x0*(1-xi)/xi*(1-x0) -2)^0.5 else 
    -.5*((xi*(1-x0))/(x0*(1-xi)) + (x0*(1-xi))/(xi*(1-x0))-2)^0.5
}

r0_func <- function(x1,x2,x3,x4){
  .5 + (x1+x2+x3+x4)/(2*(x1+x2+x3+x4)^2+1)
}


r0_func(33,23,1,1)
 



                       
                       
                     
